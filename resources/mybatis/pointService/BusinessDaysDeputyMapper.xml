<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	<!-- 营业日集合(副)(businessDaysDeputy)Mapper -->
<mapper namespace="businessDaysDeputyMapper">
    <!--mybatis ehcache缓存配置 -->
    <!-- 以下两个<cache>标签二选一,第一个可以输出日志,第二个不输出日志 不使用缓存加入useCache="false" -->
    <!-- <cache type="org.mybatis.caches.ehcache.LoggingEhcache" /> -->
    <!-- <cache type="org.mybatis.caches.ehcache.EhcacheCache"/>  -->

	<sql id="Base_Column_List">
    psbd_id,psbd_day ,psbd_point_service_id ,psabd_id 
    </sql>

    <resultMap type="com.op.entity.pointService.BusinessDaysDeputy" id="BusinessDaysDeputyReturnMap">
		<!-- id -->
      <id column="psbd_id" property="psbd_id"/>
		<!-- 营业日 -->
      <result column="psbd_day" property="psbd_day"/>
		<!-- 地点服务ID -->
      <result column="psbd_point_service_id" property="psbd_point_service_id"/>
		<!-- 地点服务营业日ID -->
      <result column="psabd_id" property="psabd_id"/>
   </resultMap>
   
   <!-- 根据地点服务营业日ID获取数据 -->
  	<select id="getDataByPsId" resultType="com.op.entity.pointService.BusinessDaysDeputy" parameterType="String">
  		select 
  			psbd_id,psbd_day ,psbd_point_service_id ,psabd_id 
  		from 
  			BusinessDaysDeputy
  		where
  			psabd_id = #{psabd_id}	
  	</select>
	
</mapper>